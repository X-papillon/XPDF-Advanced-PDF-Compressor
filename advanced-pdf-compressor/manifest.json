{
  "manifest_version": 3,
  "name": "Advanced PDF Compressor",
  "description": "Compress PDF files directly in your browser with high quality.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
  {
    "resources": ["pdf.worker.js"],
    "matches": ["<all_urls>"]
  }
],
  "permissions": ["storage"],
  "icons": {
    "128": "128.png",
	"48": "48.png",
	"16": "16.png"
  }
}
